}

@article{bikos_qualitative_2014,
	title = {A {Qualitative} {Evaluation} of the 4-{H} {Record} {Book} in {Light} of the {PYD} {Concepts}: {Belonging}, {Mastery}, {Independence}, and {Generosity}},
	volume = {9},
	copyright = {Copyright (c) 2014 Journal of Youth Development},
