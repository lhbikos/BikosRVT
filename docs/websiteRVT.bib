	year = {2016},
	doi = {10.1037/14840-012},
	keywords = {Curriculum, Psychology Education, Undergraduate Education, Study Abroad, activities, assignments, Cross Cultural Psychology, Educational Objectives, internationalizing the undergraduate psychology curriculum, learning goals, off-campus international experience, on-campus coursework, on-campus research},
	pages = {199--216},
	file = {Bikos et al. - 2016 - Integrating off-campus international experience in.pdf:C\:\\Users\\lhbikos\\Zotero\\storage\\F6XXMVIP\\Bikos et al. - 2016 - Integrating off-campus international experience in.pdf:application/pdf}
