	pages = {33--79},
	file = {SAGE PDF Full Text:C\:\\Users\\lhbikos\\Zotero\\storage\\7SUH3KQ4\\Bikos et al. - 2021 - The Self-Efficacy for Sociocultural Adaptation Sca.pdf:application/pdf}
