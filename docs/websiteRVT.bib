@article{bikos_effects_2015,
	title = {Effects of searching, faith, and time on the presence of calling in international service learning returnees},
	volume = {34},
	issn = {0733-4273},
	abstract = {We examined the effects of searching for calling, strength of faith, and time (in months) on the presence of calling among returnees from a faith-based international service-learning (ISL) program. Participants were 74 undergraduate students (M age = 19.79; 8\% female; 67\% Caucasian) whose near month-long (M = 84 months) immersion took them to host countries such as Rwanda, Vietnam, and Haiti. Students completed online surveys at pre-departure and at re-entry intervals of 2-weeks and 6-months. Data were analyzed using hierarchical linear modeling where person was the clustering variable. Presence of calling was identified by a quadratic function of time, peaking between 3- and 4-months re-entry and highest for those who are persistent in searching and have the strongest aggregate faith. Although study abroad researchers have long claimed re-entry effects (e.g., re-entry culture shock), our research is, perhaps, the first empirical evidence of a significant quadratic function for ISL returnees. Programmatic recommendations include pre-departure orientation and re-entry services that include framing the career decision-making process as one of discernment and incorporating theologically informed resources for faith-based programs such as these. (PsycINFO Database Record (c) 2018 APA, all rights reserved)},
	number = {4},
	journal = {Journal of Psychology and Christianity},
	author = {Bikos, Lynette H. and Rodney, Heather and Gowen, Melissa and Yamamoto, Mari and Dykhouse, Elizabeth C.},
	year = {2015},
	keywords = {College Students, Service Learning, Educational Programs, Faith, Faith Based Organizations, faith-based programs, international service-learning, time, undergraduate students},
	pages = {325--339},
	file = {Bikos et al. - 2015 - Effects of searching, faith, and time on the prese.pdf:C\:\\Users\\lhbikos\\Zotero\\storage\\2M72UQBD\\Bikos et al. - 2015 - Effects of searching, faith, and time on the prese.pdf:application/pdf}
}

@article{de_paul_perceived_2015,
	title = {Perceived organizational support: {A} meaningful contributor to expatriate development professionals’ psychological well-being},
	volume = {49},
	issn = {0147-1767},
	shorttitle = {Perceived organizational support},
	doi = {10.1016/j.ijintrel.2015.06.004},
	abstract = {Expatriate health care and humanitarian aid professionals (N = 159) who were currently deployed internationally participated in this study. We used organizational support theory to guide our examination of the associations between sociocultural adaptation difficulty, perceived organizational support (from organizational headquarters [home] and its foreign subsidiary [host]), and expatriates’ psychological well-being while deployed. Snowball and social networking techniques were used to recruit globally dispersed participants. Participants were mostly female (59\%), represented 26 nationalities, and nearly all (99\%) were actively working within 53 international settings. Multiple regression analyses indicated that sociocultural adaptation and perceived organizational support from the host and home organizations made significant contributions to expatriates’ psychological well-being. Perceived organizational support from host organization was most strongly associated with expatriates’ psychological well-being while deployed (R² = .28, p = .000; β [host] = .39, p = .000; β [home] = .12, p = .10). We suggest that host and home organizations employing expatriates work collaboratively to monitor their expatriates’ psychological well-being, perception of the adequacy of support provided, and communicate to provide integrated resources in response to expatriates’ dynamic needs. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
	journal = {International Journal of Intercultural Relations},
	author = {De Paul, Nicola F. and Bikos, Lynette H.},
	month = nov,
	year = {2015},
	keywords = {Employee Attitudes, Expatriates, Well Being, Sociocultural adaptation, Expatriate, Humanitarian aid, Perceived organizational support, Psychological well-being},
	pages = {25--32},
	file = {De Paul and Bikos - 2015 - Perceived organizational support A meaningful con.pdf:C\:\\Users\\lhbikos\\Zotero\\storage\\SEV9TZ53\\De Paul and Bikos - 2015 - Perceived organizational support A meaningful con.pdf:application/pdf}
}

