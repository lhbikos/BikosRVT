	pages = {605--609},
	file = {EBSCO Full Text:C\:\\Users\\lhbikos\\Zotero\\storage\\S6HURRAK\\Bikos and Lewis Hall - 2009 - Psychological functioning of international mission.pdf:application/pdf}
}

